  {
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "SDK测试管理平台"
    },
    "basePath": "",
    "tags": [
      {
        "name": "TC_MANAGE",
        "description": "测试用例管理"
      },
      {
        "name": "PARAM_MANAGE",
        "description": "参数配置管理"
      },
      {
        "name": "TEST_PLAN",
        "description": "测试计划"
      }
    ],
    "schemes": [
      "http"
    ],
    "paths": {
      "/sdk/version": {
        "get": {
          "tags": [
            "TC_MANAGE"
          ],
          "summary": "查询SDK版本",
          "operationId": "querySDKVersion",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "query",
              "name": "version"
            }
          ],
          "responses": {
            "200": {
              "description": "ok",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/SDKVersion"
                }
              }
            }
          }
        }
      },
      "/sdk/interface": {
        "get": {
          "tags": [
            "TC_MANAGE"
          ],
          "summary": "查询SDK接口",
          "operationId": "querySDKInterface",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "query",
              "name": "name"
            }
          ],
          "responses": {
            "200": {
              "description": "ok",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/SDKInterface"
                }
              }
            }
          }
        }
      },
      "/testcase/config": {
        "get": {
          "tags": [
            "PARAM_MANAGE"
          ],
          "summary": "查询测试用例配置",
          "operationId": "queryTestcaseConfig",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "query",
              "name": "name"
            },
            {
              "in": "query",
              "name": "compatibleInterface"
            }
          ],
          "responses": {
            "200": {
              "description": "ok",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/TestcaseConfig"
                }
              }
            }
          }
        }
      },
      "/stock_class": {
        "get": {
          "tags": [
            "PARAM_MANAGE"
          ],
          "summary": "查询股票类型",
          "operationId": "queryStockClass",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "query",
              "name": "classID"
            }
          ],
          "responses": {
            "200": {
              "description": "ok",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/StockClass"
                }
              }
            }
          }
        }
      },
      "/test_env": {
        "get": {
          "tags": [
            "PARAM_MANAGE"
          ],
          "summary": "查询测试环境配置",
          "operationId": "queryTestEnv",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "query",
              "name": "envType",
              "type": "string",
              "enum": [
                "Test",
                "Simulation",
                "Production"
              ]
            }
          ],
          "responses": {
            "200": {
              "description": "ok",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/TestEnv"
                }
              }
            }
          }
        }
      },
      "/test_plan": {
        "get": {
          "tags": [
            "TEST_PLAN"
          ],
          "summary": "测试计划",
          "operationId": "queryTestplan",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "in": "query",
              "name": "name"
            }
          ],
          "responses": {
            "200": {
              "description": "ok",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/TestPlan"
                }
              }
            }
          }
        }
      }
