    "definitions": {
      "SDKVersion": {
        "type": "object",
        "properties": {
          "uid": {
            "type": "string"
          },
          "version": {
            "type": "string"
          },
          "iteration": {
            "type": "string"
          },
          "platform": {
            "type": "string",
            "enum": [
              "Android",
              "iOS"
            ]
          },
          "resourceURL": {
            "type": "string"
          },
          "uploadTime": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "SDKInterface": {
        "type": "object",
        "properties": {
          "uid": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "sdkVersion": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "platform": {
            "type": "string",
            "enum": [
              "Android",
              "iOS"
            ]
          },
          "function": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "TestcaseConfig": {
        "type": "object",
        "description": "测试用例参数",
        "properties": {
          "uid": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "compatibleInterface": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "stockCodes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "extraParams": {
            "type": "string"
          }
        }
      },
      "StockClass": {
        "type": "object",
        "description": "股票分类",
        "properties": {
          "classID": {
            "type": "string"
          },
          "className": {
            "type": "string"
          },
          "stockCodes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "TestEnv": {
        "type": "object",
        "description": "测试环境",
        "properties": {
          "envType": {
            "type": "string",
            "enum": [
              "Test",
              "Simulation",
              "Production"
            ]
          },
          "sites": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "ip": {
                  "type": "string"
                }
              }              
            }
          }
        }
      },
      "TestPlan": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "pattern": {
            "type": "string",
            "enum": [
              "AndroidAndiOS",
              "TestEnv",
              "Competition"
            ]
          },
          "marketLevel": {
            "type": "string"
          },
          "marketPerms": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "casesConfig": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "testcaseConfig": {
                  "$ref": "#/definitions/TestcaseConfig"
                },
                "roundTimes": {
                  "type": "number"
                },
                "roundIntervalSec": {
                  "type": "number"
                },
                "enable": {
                  "type": "boolean"
                }                
              }
            }
          },
          "comparatorArgs": {
            "type": "object",
            "properties": {
              "AndroidAndiOS": {
                "type": "object",
                "properties": {
                  "versionAndroid": {
                    "type": "string"
                  },
                  "versioniOS": {
                    "type": "string"
                  },
                  "testEnv": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "ip": {
                          "type": "string"
                        }
                      }              
                    }
                  }
                }
              },
              "TestEnv": {
                "type": "object",
                "properties": {
                  "sdkVersion": {
                    "type": "string"
                  },
                  "testEnvs": {
                    "type": "array",
                    "items": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "name": {
                            "type": "string"
                          },
                          "ip": {
                            "type": "string"
                          }
                        }              
                      }
                    }
                  }
                }   
              },
              "Competition": {
                "type": "object",
                "properties": {
                  "sdkVersion": {
                    "type": "string"
                  },
                  "testEnv": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "ip": {
                          "type": "string"
                        }
                      }              
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }